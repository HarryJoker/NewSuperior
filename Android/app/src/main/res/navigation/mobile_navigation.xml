<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="gov.android.com.superior.ui.unit.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_masses"
        android:name="gov.android.com.superior.ui.unit.masses.MassesFragment"
        android:label="@string/title_masses"
        tools:layout="@layout/fragment_masses" />

    <fragment
        android:id="@+id/navigation_person"
        android:name="gov.android.com.superior.ui.unit.person.PersonFragment"
        android:label="@string/title_person"
        tools:layout="@layout/fragment_person" />
</navigation>